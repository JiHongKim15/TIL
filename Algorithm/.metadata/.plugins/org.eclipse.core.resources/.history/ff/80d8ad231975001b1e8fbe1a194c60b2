import java.util.Scanner;

public class 사회망서비스_2533 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		
		int[][] sns = new int[N+1][N+1];
		
		for (int i = 0; i < N-1; i++) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			
			sns[a][b] = 1;
			sns[b][a] = 1;
		}
		
		
	}
}
