import java.util.Scanner;

/**
 * 백준
 * @author 15com
 *
 */
public class 뱀_3190 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N =sc.nextInt();
		int K = sc.nextInt();
		int[][] apple = new int[K][2];
		int[][] map = new int[N][N];
		
		for (int i = 0; i < K; i++) {
			apple[i][0] = sc.nextInt();
			apple[i][1] = sc.nextInt();
		}
		
		int l = sc.nextInt();
		
		int[][] L = new int[l][2];
		
		for (int i = 0; i < l; i++) {
			L[i][0] = sc.nextInt();
		}
	}
}
