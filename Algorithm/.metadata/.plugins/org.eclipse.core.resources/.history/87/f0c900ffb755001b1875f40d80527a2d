import java.util.Scanner;

public class 스티커_9465 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		int[] dr = {0, 0, 1, -1};
		int[] dc = {1, -1, 0, 0};
		for (int tc = 0; tc < T; tc++) {
			int n = sc.nextInt();
			int[][] sticky = new int[2][n];
			
			for (int i = 0; i < 2; i++) {
				for (int j = 0; j < n; j++) {
					sticky[i][j] = sc.nextInt();
				}
			}
			
			
		}
	}
}
