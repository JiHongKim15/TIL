import java.util.Scanner;

public class 암호제작_1837 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String P = sc.next();
		String K = sc.next();
	
		long i;
		for (i = 2; i < K; i++) {
		
			if( P % i == 0) {
				System.out.println("BAD " + i);
				break;
			}
		
		}
		
		if(i >= K) System.out.println("GOOD");
				
				
	}
	
}
