import java.util.ArrayList;
import java.util.Scanner;

public class 평범한배낭_12865 {
	
	static class Node{
		int W;
		int V;
		
		Node(int W, int V){
			this.W = W;
			this.V = V;
		}
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int K = sc.nextInt();
		
		
//		ArrayList<Node> al = new ArrayList<>();
		int[] W = new int[N];
		int[] V = new int[N];
		
		
		for (int i = 0; i < N; i++) {
//			al.add(new Node(sc.nextInt(), sc.nextInt()));
			W[i] = sc.nextInt();
			V[i] = sc.nextInt();
		}
		
		int max = 0;
		
		for(int i=0; i< (1 << N); i++) {
			int sum = 0;
			for(int j=0; j<N ; j++) {
				if( (i & (1 << j)) != 0 ) {
					//j번째 원소 선택
					sum += nums[j];
				}
				
			}
			sum = Math.max(sum, max);
			System.out.print(" ");
		}
		
	}
}
