import java.util.ArrayList;
import java.util.Scanner;

public class Æò¹üÇÑ¹è³¶_12865 {
	
	static class Node{
		int W;
		int K;
		
		Node(int W, int K){
			this.W = W;
			this.K = K;
		}
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int K = sc.nextInt();
		
		
		ArrayList<Node> al = new ArrayList<>();
		
		
		for (int i = 0; i < N; i++) {
			
			al.add(new Node(sc.nextInt(), sc.nextInt()));
		}
		
		
	}
}
