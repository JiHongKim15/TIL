import java.util.Scanner;

/**
 * 백준
 * @author 15com
 *
 */
public class 뱀_3190 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N =sc.nextInt();
		int K = sc.nextInt();
		int[][] apple = new int[K][2];
		
		for (int i = 0; i < K; i++) {
			apple[i] = sc.nextInt();
		}
	}
}
