import java.util.Scanner;

public class 회장뽑기_2660 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		
		int[][] board = new int[N+1][N+1];
		
		while(true) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			if(a == -1 | b == -1)
				break;
			
			
		}
	}
}
