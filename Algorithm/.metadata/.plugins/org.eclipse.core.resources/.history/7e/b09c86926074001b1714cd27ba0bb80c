import java.util.Scanner;

public class 숫자고르기_2668 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int[] arr = new int[N + 1];
		for (int i = 1; i <= N; i++) {
			arr[i] = sc.nextInt();
		}

		for (int i = 1; i <= arr.length; i++) {
			dfs(arr, i);
		}
	}

	private static void dfs(int[] arr, int i) {
		if(i >= arr.length)
		
		dfs(arr, arr[i]);

	}

}
